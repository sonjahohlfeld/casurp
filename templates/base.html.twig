<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}casurp{% endblock %}</title>
    {% block stylesheets %}
        {# Global styles bundled with webpack #}
        <link rel="stylesheet" href="{{ asset('build/base.css') }}" />
    {% endblock %}
    {% block head %}{% endblock %}
</head>
<body class="bg-dark">
{% block javascripts %}
    {# Global dependencies bundled with webpack #}
    <script src="{{ asset('build/app.js') }}"></script>
    <script src="{{ asset('build/base.js') }}"></script>
    <script src="{{ asset('build/products.js') }}"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
{% endblock %}
{% block body %}{% endblock %}
</body>
</html>